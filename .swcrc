{
  "$schema": "https://swc.rs/schema.json",
  "jsc": {
    "target": "es2024",
    "loose": true,
    "minify": {
      "mangle": true,
      "compress": {
        "defaults": true,
        "arguments": true,
        "passes": 3
      }
    },
    "baseUrl": "./dist",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "test": ".*\\.js$",
  "minify": true
}
