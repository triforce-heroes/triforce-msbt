{
  "name": "@triforce-heroes/triforce-msbt",
  "description": "Transpile and rebuild MSBT files.",
  "version": "1.0.0",
  "license": "Apache License 2.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf ./dist && tspc -p ./tsconfig.build.json && swc ./dist -d .",
    "prettier": "prettier --check ./src ./tests",
    "prettier:fix": "prettier --write ./src ./tests",
    "eslint": "eslint ./src ./tests",
    "eslint:fix": "eslint --fix ./src ./tests",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@rheactor/eslint-config-rheactor": "github:rheactor/eslint-config-rheactor",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.3",
    "@triforce-heroes/triforce-core": "github:triforce-heroes/triforce-core",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-plugin-vitest": "^0.5.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.4",
    "ts-patch": "^3.3.0",
    "typescript": "^5.9.2",
    "typescript-transform-paths": "^3.5.5",
    "vitest": "^3.2.4"
  }
}
